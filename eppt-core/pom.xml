	<build>
		<plugins>
<!--			<plugin>-->
<!--				<groupId>org.apache.maven.plugins</groupId>-->
<!--				<artifactId>maven-surefire-plugin</artifactId>-->
<!--&lt;!&ndash;				<version>2.19.1</version>&ndash;&gt;-->
<!--				<configuration>-->
<!--&lt;!&ndash;					<workingDirectory>${project.basedir}</workingDirectory>&ndash;&gt;-->
<!--&lt;!&ndash;					<forkMode>once</forkMode>&ndash;&gt;-->
<!--&lt;!&ndash;					<argLine>-Djava.library.path=${project.build.directory}/lib</argLine>&ndash;&gt;-->
<!--&lt;!&ndash;					<argLine>-Djava.library.path=${project.build.directory}\lib</argLine>&ndash;&gt;-->
<!--				</configuration>-->
<!--			</plugin>-->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.22.1</version>
				<configuration>
					<forkCount>3</forkCount>
					<reuseForks>true</reuseForks>
					<argLine>-Xmx1024m -XX:MaxPermSize=256m -Djava.library.path=${project.build.directory}/lib</argLine>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<version>3.1.1</version>
				<executions>
					<execution>
						<id>copy-dependencies</id>
						<phase>validate</phase>
						<goals>
							<goal>copy</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<artifactItems>
						<artifactItem>
							<groupId>hec</groupId>
							<artifactId>javaHeclib</artifactId>
							<version>7-GS</version>
							<type>dll</type>
							<destFileName>javaHeclib.dll</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>dwr</groupId>
							<artifactId>callite</artifactId>
							<version>v16</version>
							<type>dll</type>
							<destFileName>CalLiteV16.dll</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>dwr</groupId>
							<artifactId>callite-xa</artifactId>
							<version>v16</version>
							<type>lic</type>
							<destFileName>CalLiteXAv16.lic</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>sunsetsoft</groupId>
							<artifactId>xa</artifactId>
							<version>v16</version>
							<classifier>x64</classifier>
							<type>dll</type>
							<destFileName>XAV16_x64.dll</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>dwr</groupId>
							<artifactId>n2xa</artifactId>
							<version>v3</version>
							<type>dll</type>
							<destFileName>n2xav3.dll</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>dwr</groupId>
							<artifactId>cb</artifactId>
							<version>v16</version>
							<type>dll</type>
							<destFileName>cb.dll</destFileName>
						</artifactItem>
					</artifactItems>
					<outputDirectory>${project.build.directory}/lib</outputDirectory>
					<overWriteReleases>true</overWriteReleases>
					<overWriteSnapshots>true</overWriteSnapshots>
				</configuration>
			</plugin>
		</plugins>
	</build>
