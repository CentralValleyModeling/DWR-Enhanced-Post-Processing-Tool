	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.netbeans.utilities</groupId>
				<artifactId>nbm-maven-plugin</artifactId>
				<version>4.5</version>
				<extensions>true</extensions>
				<configuration>
					<verifyRuntime>skip</verifyRuntime>
					<nbmResources>
						<nbmResource>
							<directory>${project.build.directory}/lib</directory>
							<targetPath>modules/lib</targetPath>
							<includes>
								<include>**</include>
							</includes>
						</nbmResource>
						<nbmResource>
							<directory>${project.basedir}/src/main/config</directory>
							<targetPath>config</targetPath>
							<includes>
								<include>**</include>
							</includes>
						</nbmResource>
						<nbmResource>
							<directory>${project.basedir}/src/main/lib</directory>
							<targetPath>lib</targetPath>
							<includes>
								<include>**</include>
							</includes>
						</nbmResource>
						<nbmResource>
							<directory>${project.basedir}/src/main/wresl</directory>
							<targetPath>wresl</targetPath>
							<includes>
								<include>**</include>
							</includes>
						</nbmResource>
						<nbmResource>
							<directory>${project.basedir}/src/main/jasper</directory>
							<targetPath>jasper</targetPath>
							<includes>
								<include>**</include>
							</includes>
						</nbmResource>
						<nbmResource>
							<directory>${project.basedir}/src/main/docs</directory>
							<targetPath>docs</targetPath>
							<includes>
								<include>**</include>
							</includes>
						</nbmResource>
					</nbmResources>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<archive>
						<manifestFile>${project.build.outputDirectory}/META-INF/MANIFEST.MF</manifestFile>
					</archive>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<version>3.1.1</version>
				<executions>
					<execution>
						<id>copy-dependencies</id>
						<phase>validate</phase>
						<goals>
							<goal>copy</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>dwr</groupId>
									<artifactId>callite</artifactId>
									<version>v16</version>
									<type>dll</type>
									<destFileName>CalLiteV16.dll</destFileName>
								</artifactItem>
								<artifactItem>
									<groupId>dwr</groupId>
									<artifactId>callite-xa</artifactId>
									<version>v16</version>
									<type>lic</type>
									<destFileName>CalLiteXAv16.lic</destFileName>
								</artifactItem>
								<artifactItem>
									<groupId>sunsetsoft</groupId>
									<artifactId>xa</artifactId>
									<version>v16</version>
									<type>dll</type>
									<destFileName>XAV16_x64.dll</destFileName>
								</artifactItem>
								<artifactItem>
									<groupId>dwr</groupId>
									<artifactId>n2xa</artifactId>
									<version>v3</version>
									<type>dll</type>
									<destFileName>n2xav3.dll</destFileName>
								</artifactItem>
								<artifactItem>
									<groupId>dwr</groupId>
									<artifactId>cb</artifactId>
									<version>v16</version>
									<type>dll</type>
									<destFileName>cb.dll</destFileName>
								</artifactItem>
							</artifactItems>
							<outputDirectory>${project.build.directory}/lib</outputDirectory>
							<overWriteReleases>true</overWriteReleases>
							<overWriteSnapshots>true</overWriteSnapshots>
						</configuration>
					</execution>
					<execution>
						<id>download-javaHeclib</id>
						<phase>validate</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>mil.army.usace.hec</groupId>
									<artifactId>javaHeclib</artifactId>
									<version>7-IP-6-win-x86_64</version>
									<type>zip</type>
								</artifactItem>
							</artifactItems>
							<outputDirectory>${project.build.directory}/lib</outputDirectory>
							<overWriteReleases>true</overWriteReleases>
							<overWriteSnapshots>true</overWriteSnapshots>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.22.1</version>
			</plugin>
		</plugins>
	</build>
